{
  "name": "gitpedia",
  "description": "Basic Git learning app",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Muhammad Akmal Hisyam",
    "email": "muhammad.akmal@student.umn.ac.id"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production react-scripts start",
    "dev": "cross-env NODE_ENV=development react-scripts start",
    "build": "cross-env NODE_ENV=production react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)'",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint src --fix && yarn format",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "4.7.0",
    "@capacitor/haptics": "4.1.0",
